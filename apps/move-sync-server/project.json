{
  "name": "move-sync-server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/move-sync-server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/move-sync-server",
        "main": "apps/move-sync-server/src/main.ts",
        "tsConfig": "apps/move-sync-server/tsconfig.app.json",
        "webpackConfig": "apps/move-sync-server/webpack.config.js",
        "assets": ["apps/move-sync-server/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/move-sync-server/src/environments/environment.ts",
              "with": "apps/move-sync-server/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "move-sync-server:build",
        "host": "0.0.0.0"
      },
      "configurations": {
        "production": {
          "buildTarget": "move-sync-server:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/move-sync-server/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/move-sync-server"],
      "options": {
        "jestConfig": "apps/move-sync-server/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
